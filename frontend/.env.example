# HydrantHub Frontend Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to Git!

# Development API URL
VITE_API_URL=http://localhost:5000

# Production API URL (uncomment for production)
# VITE_API_URL=https://your-production-api.railway.app

# App Configuration
VITE_APP_NAME=HydrantHub
VITE_APP_VERSION=1.0.0

# Map Configuration (optional)
VITE_DEFAULT_MAP_CENTER_LAT=43.5890
VITE_DEFAULT_MAP_CENTER_LON=-79.6441
VITE_DEFAULT_MAP_ZOOM=12
